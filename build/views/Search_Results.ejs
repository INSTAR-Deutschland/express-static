<% include _partials/template/_head_NO_WRAP.ejs %><body> <% include _partials/template/_outdated.ejs %><header><% include _partials/template/_header.ejs %></header> <!-- #############################################  Search Results Header ############################################# --><section class="slanding"><div class="container" <div class="row"> <% if (response.hits.total > 0) { %><h1 class="headline">Search Results</h1><hr><br/><h2 class="subheader">Run took <%= response.took %> milliseconds</h2><h5 class="text-muted">Max search score: "<%= response.hits.max_score %>"</h5> <% } else { %><h1 class="headline hidden-xs-down">No results found!</h1><h2 class="headline hidden-sm-up">No Results<h2><br/><h2 class="subheader hidden-xs-down">¯\_(ツ)_/¯ Your keyword produced no hits ¯\_(ツ)_/¯</h2><h3 class="subheader hidden-sm-up">¯\_(ツ)_/¯</h3> <% } %><h2 class="subheader"><i class="fa fa-angle-double-down" aria-hidden="true"></i></h2><br/></div></div></section><section class="container" style="margin-bottom: 200px;"><div class="row"> <% if (response.hits.total > 0) { %><div class="card-columns mb"> <% for(var i=0; i<response.hits.hits.length;i++){ if (i === 8) { break; } %><div class="card card-inverse shadow mt" style="background-color: #333; border-color: #333;"> <img class="card-img-top img-fluid" src="<%= response.hits.hits[i]._source.image %>" alt="Card image cap"><div class="card-block" style="height: 215px;"><h4 class="card-title"><%= response.hits.hits[i]._source.title %></h4><p class="card-text"><%= response.hits.hits[i]._source.abstract %></p> <a href="<%= response.hits.hits[i]._source.link %>" class="btn btn-primary">Read More</a></div></div> <% } %></div> <% } else { %><div class="card-deck-wrapper mb"><div class="card-deck"><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/P_SearchThumb_Products.jpg" alt="INSTAR Product Portfolio"><div class="card-block"><h4 class="card-title">Products</h4><p class="card-text">Get all Information about INSTAR Products. Downloads, Technical Specifications and more.</p> <a href="/Products/" class="btn btn-secondary">Read More</a></div></div><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/QI_SearchThumb_QuickInstallation.jpg" alt="INSTAR Quick Installation Guides"><div class="card-block"><h4 class="card-title">Quick Installation</h4><p class="card-text">Install your INSTAR camera with ease - read this guide for more information on the process.</p> <a href="/Quick_Installation/" class="btn btn-secondary">Read More</a></div></div><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/IA_SearchThumb_InternetAccess.jpg" alt="Access your INSTAR Camera over the Internet"><div class="card-block"><h4 class="card-title">Internet Access</h4><p class="card-text">All INSTAR cameras can be accessed from the internet - click here to learn more about DDNS, P2P &amp; Co.</p> <a href="/Internet_Access/" class="btn btn-secondary">Read More</a></div></div></div></div><div class="card-deck-wrapper mt"><div class="card-deck"><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/MD_SearchThumb_MDSetup.jpg" alt="Motion Detection"><div class="card-block"><h4 class="card-title">Motion Detection</h4><p class="card-text">Use your INSTAR camera to notify you, whenever someone enters the surveillance areas!</p> <a href="/Motion_Detection/" class="btn btn-secondary">Read More</a></div></div><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/AU_SearchThumb_ForDeveloper.jpg" alt="Advanced Users"><div class="card-block"><h4 class="card-title">Advanced User</h4><p class="card-text">IP cameras are small computers that can be tinkered with. Advanced users, programmers... read on!</p> <a href="/Advanced_User/" class="btn btn-secondary">Read More</a></div></div><div class="card shadow"> <img class="card-img-top img-fluid" src="/images/Search/P_SearchThumb_WebShop.jpg" alt="INSTAR Webshop"><div class="card-block"><h4 class="card-title">INSTAR Shop</h4><p class="card-text">Interested in getting a new IP camera? Or looking for some accessories for your existing one?</p> <a href="http://instar.de" class="btn btn-secondary" target="_blank">Go Shopping</a></div></div></div></div> <% } %></div></section> <!-- #############################################  Social ############################################# --><section class="products"><% include _partials/content/_products.ejs %></section><section class="social"><% include _partials/content/_social.ejs %></section> <!-- ############################################# Footer ############################################# --><footer><% include _partials/template/_footer.ejs %></footer> <% include _partials/template/_jsdefaults_NO_WRAP.ejs %></section></body></html>